{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "reST",
  "patterns": [
    {
      "include": "#heading"
    },
    {
      "include": "#href"
    },
    {
      "include": "#yyyyMMdd"
    },
    {
      "include": "#HHmmss"
    },
    {
      "include": "#dummy"
    }
  ],
  "repository": {
    "dummy": {
      "patterns": [
        {
          "match": "dfkjfkdajiujadgaerhgegl",
          "name": "markup.bold"
        }
      ]
    },
    "heading": {
      "patterns": [
        {
          "match": "^((=|-|#){4,})$",
          "captures": {
            "1": {
              "name": "markup.heading"
            }
          }
        }
      ]
    },
    "href": {
      "patterns": [
        {
          "name": "markup.underline.link",
          "begin": "(?<!`)`[^_`][^`]+",
          "end": "`_"
        }
      ]
    },
    "yyyyMMdd": {
      "patterns": [
        {
          "match": "([0-9]{4}-[0-9]{2}-[0-9]{2})",
          "captures": {
            "1": {
              "name": "constant.other"
            }
          }
        }
      ]
    },
    "HHmmss": {
      "patterns": [
        {
          "match": "([0-9]{2}:[0-9]{2}:[0-9]{2})",
          "captures": {
            "1": {
              "name": "constant.other"
            }
          }
        }
      ]
    }
  },
  "scopeName": "text.reST"
}
